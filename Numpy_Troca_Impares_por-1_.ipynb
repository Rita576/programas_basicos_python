        "import numpy as np\n",
        "\n",
        "arr = np.arange(10)\n",
        "num = arr[:]\n",
        "for i in num:\n",
        "  if i % 2 == 0:\n",
        "    num = i\n",
        "  else:\n",
        "    num = -1\n",
        "    # A num que foi uma copia onde mostra os impares com -1\n",
        "  print (num)\n",
        "    # A Arr que nao foi alterada\n",
        "print(arr)\n",
        "\n"
      ],
